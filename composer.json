{
  "name": "bootell/yii2-phpredis",
  "description": "Redis Cache, Session and ActiveRecord for the Yii2 Using PhpRedis",
  "keywords": ["yii2", "redis", "phpredis", "active-record", "cache", "session"],
  "license": "BSD-3-Clause",
  "require": {
    "yiisoft/yii2-redis": "~2.0.0",
    "ext-redis": ">5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "yiisoft/yii2-dev": "^2.0"
  },
  "autoload": {
    "psr-4": { "bootell\\Yii2\\redis\\": "src" }
  },
  "autoload-dev": {
    "psr-4": { "yiiunit\\extensions\\redis\\": "tests/"}
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
